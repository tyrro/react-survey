{
  "name": "react-survey",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:coverage": "rm -f ./.nyc_output/out.json && yarn test --coverage --watchAll=false && yarn cypress:run && node ./scripts/coverage-merge.js && nyc report",
    "test:coverage-merge": "node ./scripts/coverage-merge.js"
  },
  "nyc": {
    "report-dir": "coverage/cypress",
    "excludeAfterRemap": true
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.12",
    "@nimblehq/eslint-config-nimble": "2.2.1",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "autoprefixer": "10.4.4",
    "babel-plugin-istanbul": "6.1.1",
    "cypress": "9.5.2",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-cypress": "2.12.1",
    "husky": "6",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "nyc": "15.1.0",
    "postcss": "8.4.12",
    "prettier": "2.6.0",
    "sass": "1.49.9",
    "stylelint": "14.6.0",
    "stylelint-config-property-sort-order-smacss": "9.0.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.2.0",
    "tailwindcss": "3.0.23"
  },
  "engines": {
    "node": "^12.22.8",
    "yarn": "^1.22.17"
  }
}
